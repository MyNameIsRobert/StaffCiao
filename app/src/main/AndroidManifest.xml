<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roc06.staffciao">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".extraCountInformation"
            android:label="@string/activity2_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.roc06.staffciao.MainActivity" />
        </activity>
        <activity
            android:name=".camperDisplay"
            android:label="@string/title_activity_camperdisplay"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ScheduleDisplay"
            android:label="@string/title_activity_schedule_viewer"
            android:theme="@style/AppTheme" />

    </application>

</manifest>